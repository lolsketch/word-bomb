{
  "name": "word-dash",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "npm-run-all --parallel client:dev server:dev",
    "client:dev": "vite --port 5004",
    "client:build": "yarn typecheck && vite build",
    "client:preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "server:dev": "rm -rf .partykit && partykit dev",
    "deploy": "yarn client:build && cp words/levels.json dist/assets && partykit deploy"
  },
  "dependencies": {
    "@types/node": "^20.11.17",
    "@uidotdev/usehooks": "^2.4.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.324.0",
    "partykit": "^0.0.72",
    "partysocket": "^0.0.12",
    "react": "^18.2.0",
    "react-countdown-circle-timer": "^3.2.1",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.0"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.8",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "fs": "^0.0.1-security",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.1"
  }
}
